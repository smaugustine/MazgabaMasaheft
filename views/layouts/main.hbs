      {{#if subtitle}}<h2 class="subtitle is-5">{{subtitle}}</h2>{{/if}}

      {{#if tabs}}{{> tabs tabs branch=branch}}{{/if}}

      {{{body}}}

    </div>
  </section>

  {{> footer}}

</body>
